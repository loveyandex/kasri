<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXDialog?>
<?import com.jfoenix.controls.JFXDialogLayout?>
<?import com.lynden.gmapsfx.GoogleMapView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="anchorroot"
            id="AnchorPane"
            xmlns="http://javafx.com/javafx/8.0.65"
            fx:controller="com.amin.ui.map.LatLongFXMLController">

    <children>
        <GoogleMapView fx:id="googleMapView" layoutX="14.0" layoutY="21.0"
                       prefHeight="700.0"
                       prefWidth="1000"

        />


        <StackPane fx:id="stackpane" AnchorPane.leftAnchor="300" AnchorPane.topAnchor="200">

            <JFXDialog fx:id="dialog">
                <JFXDialogLayout>
                    <heading>
                        <Label>KNN data modeling</Label>
                    </heading>
                    <body>
                        <Label fx:id="descrip">
                        </Label>
                    </body>
                    <actions>
                        <JFXButton fx:id="acceptButton" styleClass="dialog-accept">OK
                        </JFXButton>
                    </actions>
                </JFXDialogLayout>
            </JFXDialog>


        </StackPane>


    </children>
</AnchorPane>
