<?xml version="1.0" encoding="UTF-8"?>

<?import com.amin.ui.theme.FontAwesomeIconView2?>
<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane"
            prefHeight="397.0"
            fx:id="root"
            prefWidth="419.0"
            stylesheets="/dark-theme.css"
            xmlns="http://javafx.com/javafx/8.0.141"
            fx:controller="com.amin.ui.mappless.MaplessController">
    <children>
        <JFXTextField fx:id="latitude" labelFloat="true" layoutX="95.0" layoutY="122.0" prefHeight="33.0"
                      prefWidth="259.0"
                      promptText="Latitude">
            <font>
                <Font size="16.0"/>
            </font>
        </JFXTextField>
        <JFXTextField fx:id="longitude" labelFloat="true" layoutX="97.0" layoutY="194.0"
                      onAction="#handleLoginButtonAction" prefHeight="34.0" prefWidth="259.0" promptText="Longitude">
            <font>
                <Font size="17.0"/>
            </font>
        </JFXTextField>

            <Hyperlink fx:id="hyperlink" layoutX="85.0" layoutY="242.0"
                       onAction="#hypringCityname">i know city name
            </Hyperlink>
            <Hyperlink fx:id="hyperlinkCountryName" layoutX="85.0" layoutY="259.0"
                       onAction="#hypringCuntryname">i know country name
            </Hyperlink>


        <JFXButton fx:id="cancelbtn" layoutX="106.0" layoutY="288.0" onAction="#handleCancelButtonAction"
                   prefHeight="40.0"
                   prefWidth="90.0" styleClass="login-button" text="Cancel"/>
        <JFXButton fx:id="nextbtn" layoutX="239.0" layoutY="288.0" onAction="#handleLoginButtonAction" prefHeight="40.0"
                   prefWidth="90.0" styleClass="login-button" text="next"/>
        <FontAwesomeIconView2 glyphName="APPLE" layoutX="200.0" layoutY="79.0" size="25"/>
        <FontAwesomeIconView glyphName="USER" layoutX="64.0" layoutY="149.0" size="25"/>
        <FontAwesomeIconView glyphName="KEY" layoutX="64.0" layoutY="224.0" size="25"/>

    </children>
</AnchorPane>
